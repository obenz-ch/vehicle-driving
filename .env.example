# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# GPS Tracking Service Configuration
VITE_GPS_PROVIDER=verizon
VITE_GPS_API_KEY=your_gps_api_key_here

# Supported providers: verizon, geotab, fleetComplete, samsara, teletrac
# Provider-specific API keys
VITE_VERIZON_API_KEY=your_verizon_connect_api_key
VITE_GEOTAB_API_KEY=your_geotab_api_key
VITE_FLEET_COMPLETE_API_KEY=your_fleet_complete_api_key
VITE_SAMSARA_API_KEY=your_samsara_api_key
VITE_TELETRAC_API_KEY=your_teletrac_navman_api_key

# Notification Services
VITE_EMAIL_SERVICE_API_KEY=your_sendgrid_or_ses_key
VITE_SMS_SERVICE_API_KEY=your_twilio_api_key
VITE_PUSH_NOTIFICATION_KEY=your_firebase_key

# JWT Secret for authentication
JWT_SECRET=your_jwt_secret_here

# Server Configuration
PORT=3001
NODE_ENV=development

# Real-time WebSocket Configuration
WS_PORT=3002
WS_HEARTBEAT_INTERVAL=30000

# Alert Configuration
ALERT_BATCH_SIZE=100
ALERT_PROCESSING_INTERVAL=5000

# Device Management
DEVICE_HEALTH_CHECK_INTERVAL=60000
OFFLINE_THRESHOLD_MINUTES=5

# Geofencing
GEOFENCE_CHECK_INTERVAL=10000
SPEED_LIMIT_CACHE_DURATION=3600000